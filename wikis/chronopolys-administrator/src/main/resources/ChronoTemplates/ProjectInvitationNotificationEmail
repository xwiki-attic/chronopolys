<className>XWiki.Mail</className>
<property>
<html>#set ($format="dd/MM/yyyy")
#set ($container = $project.get("container"))
#set ($style = $xwiki.chronopolys.getProjectContainerStyle($container))
$htmlheader
&lt;b&gt;Hello $username,&lt;/b&gt;&lt;br/&gt;
&lt;br/&gt;
&lt;table border="0" width="100%" style="font-size:12px;"&gt;
&lt;tr&gt;
&lt;td colspan="2"&gt;&lt;b&gt;&lt;i&gt;Your are invited to join the group &lt;a style="background-color:$style;" href="$projecturl"&gt;$project.get("name")&lt;/a&gt; (group leader : $xwiki.getLocalUserName($project.getProjectLeader(), false))&lt;/i&gt;&lt;/b&gt;&lt;/td&gt;
&lt;/tr&gt;
&lt;tr&gt;
&lt;td&gt;Description&lt;/td&gt;&lt;td&gt;$project.get("desc")&lt;/td&gt;
&lt;/tr&gt;
## &lt;tr&gt;
## &lt;td&gt;Start&lt;/td&gt;&lt;td&gt;$xwiki.formatDate($project.get("start"), $format)&lt;/td&gt;
## &lt;/tr&gt;
## &lt;tr&gt;
## &lt;td&gt;End&lt;/td&gt;&lt;td&gt;$xwiki.formatDate($project.get("end"), $format)&lt;/td&gt;
## &lt;/tr&gt;
&lt;tr&gt;&lt;td colspan="2" style="border-bottom: solid 1px #CCC;"&gt;&lt;a href="$doc.getExternalURL()" style="font-weight:bold;"&gt;Go to the group dashboard&lt;/a&gt;&lt;br/&gt;&lt;br/&gt;&lt;/td&gt;&lt;/tr&gt;
&lt;tr&gt;
&lt;td colspan="2"&gt;&lt;br/&gt;&lt;b&gt;&lt;i&gt;Group members :&lt;/i&gt;&lt;/b&gt;&lt;/td&gt;
&lt;/tr&gt;
&lt;td colspan="2"&gt;
  #set ($glist = $project.getMembers())
  &lt;table style="font-size:12px;"&gt;
    #foreach ($gitem in $glist)
    &lt;tr&gt;
      &lt;td width="180px"&gt;$xwiki.getLocalUserName($gitem, false)&lt;/td&gt;
      &lt;td&gt;        
      &lt;/td&gt;
    &lt;/tr&gt;
    #end
  &lt;/table&gt;
  &lt;br/&gt;
&lt;/td&gt;
&lt;/tr&gt;
&lt;/table&gt;
&lt;br/&gt;
$htmlfooter</html>
