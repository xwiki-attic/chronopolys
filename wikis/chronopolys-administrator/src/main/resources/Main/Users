<content>#includeMacros("ChronoCode.UserMacros")

#if($isChronoAdmin||$isChronoManager)
#includeInContext("ChronoCode.CreateUser")
#end

#macro(usersAlpha $letter)
#chronopanelheader("$letter")
#set ($sql = ", BaseObject as obj, StringProperty as prop where obj.name=doc.fullName and obj.className='XWiki.XWikiUsers' and obj.id=prop.id.id and prop.id.name='last_name' and prop.value like '$letter%'")
#foreach ($item in $xwiki.searchDocuments($sql))
   * $xwiki.getLocalUserName($item)
#end
#chronopanelfooter()
#end

