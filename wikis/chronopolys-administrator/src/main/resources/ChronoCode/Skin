<attachmentsinline.vm>#macro(dynamicsize $sz)
#if($sz &lt; 1024)
$sz bytes
#else
#set($ksz = $sz / 1024)
#if($ksz &lt; 1024)
${ksz}kb
#else
#set($Msz = $sz / 1048576)
${Msz}Mb
#end
#end
#end
